{
    "name": "@sbbw/api",
    "version": "0.0.6",
    "description": "Types for creating sbbw widgets",
    "repository": "https://github.com/SergioRibera/sbbw",
    "author": "SergioRibera",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "rimraf ./dist && rollup -c --silent && node ./post-build.cjs",
        "npm-pack": "yarn build && cd ./dist && npm pack",
        "npm-publish": "yarn build && cd ./dist && yarn publish --access public --loglevel silly",
        "generate-docs": "typedoc"
    },
    "bugs": {
        "url": "https://github.com/SergioRibera/sbbw/issues"
    },
    "homepage": "https://github.com/SergioRibera/sbbw#readme",
    "devDependencies": {
        "@babel/core": "7.18.13",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-typescript": "7.18.6",
        "@rollup/plugin-babel": "5.3.1",
        "@rollup/plugin-commonjs": "22.0.2",
        "@rollup/plugin-node-resolve": "13.3.0",
        "@rollup/plugin-sucrase": "4.0.4",
        "@rollup/plugin-typescript": "8.4.0",
        "@typescript-eslint/eslint-plugin": "5.34.0",
        "@typescript-eslint/parser": "5.34.0",
        "regenerator-runtime": "0.13.9",
        "rimraf": "3.0.2",
        "rollup": "2.78.1",
        "rollup-plugin-terser": "7.0.2",
        "typedoc": "^0.23.10",
        "typedoc-plugin-markdown": "^3.13.4",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "tslib": "^2.4.0"
    }
}
